// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model News {
  id    String   @id @default(uuid())
  date  DateTime
  title String
  url   String
}

model Suggestions {
  id    String   @id @default(uuid())
  image String
  title String
  url   String
  date  DateTime
}
